{
    "name": "baliblissed",
    "version": "1.0.0",
    "description": "A modern, responsive, and professional website for BaliBlissed, a local Balinese travel agency specializing in authentic cultural experiences, adventure tours, and luxury vacation packages.",
    "main": "js/main.js",
    "scripts": {
        "start": "live-server",
        "format": "prettier --write .",
        "lint": "eslint js/main.js",
        "build:js": "terser js/main.js -o js/main.min.js --compress --mangle",
        "build:css": "cssnano css/style.css > css/style.min.css",
        "build": "npm-run-all build:css build:js"
    },
    "keywords": [
        "bali",
        "travel",
        "tours",
        "vacation",
        "html",
        "css",
        "javascript"
    ],
    "author": "BaliBlissed Travel Agency",
    "license": "All Rights Reserved",
    "devDependencies": {
        "cssnano-cli": "^1.0.5",
        "eslint": "^8.57.0",
        "live-server": "^1.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.3.2",
        "terser": "^5.31.1"
    }
}
